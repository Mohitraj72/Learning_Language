#include <stdio.h>
void main (){
  int profit, loss, sp, cp;
  printf("Enter your Cost price: ");
  scanf("%d", &cp);

  printf("enter your Selling Price: ");
  scanf("%d", &sp);

  profit = sp - cp;
  loss = cp - sp;

  if (sp=<cp){
    printf("profit %d", &profit);

  }
  else (cp>=sp){
    printf("loss %d", &loss);
  }
}